require("helpers")
require("ui")
require("fog")

MOD_NAME = "D&D Helper"


function onLoad()
	local startTime = os.time()
	logf("Loading module %s", MOD_NAME)

	setupUI()

	debug()

	local endTime = os.time()
	logf("Loading complete! Enjoy. (%.0f ms)", (endTime - startTime) * 1000)
end

function debug()
	spawnFog()
end
