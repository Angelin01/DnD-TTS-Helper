require("helpers")

function printf(format, ...)
	print(string.format(format, ...))
end

function logf(format, ...)
	log(string.format(format, ...))
end

function printToGM(format, ...)
	printToColor(string.format(format, ...), "Black", Color.Purple)
end

function destroyObjects(objs)
	for i = 1, #objs do
		destroyObject(objs[i])
	end
end
